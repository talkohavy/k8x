{
  "name": "k8x",
  "version": "1.0.2",
  "description": "A cli tool for switching between kubernetes contexts",
  "main": "index.js",
  "private": true,
  "bin": "./index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format-check": "prettier . --check",
    "format-fix": "prettier . --write",
    "test": "node --test",
    "typecheck": "tsc -p tsconfig.json",
    "build": "node ./build.config.mjs",
    "build:dev": "export NODE_ENV=development && node ./build.config.mjs",
    "dev": "npm run build:dev && node dist/index.js"
  },
  "contributors": ["Tal Kohavy <talkohavy@gmail.com> (@Tal Kohavy)"],
  "keywords": ["kubernetes", "cli", "k8s", "k8x"],
  "author": "Tal Kohavy <talkohavy@gmail.com> (https://github.com/talkohavy)",
  "repository": {
    "type": "git",
    "url": "https://github.com/talkohavy/k8x"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "restricted"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.1",
    "@eslint/js": "^9.34.0",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.13.0",
    "esbuild": "^0.25.5",
    "eslint": "^9.34.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "globals": "^16.3.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  },
  "dependencies": {
    "@inquirer/prompts": "^6.0.1"
  }
}
